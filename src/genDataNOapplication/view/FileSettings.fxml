<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="genDataNOapplication.view.FileSettingsController">
   <children>
      <Label layoutX="414.0" layoutY="72.0" text="File Settings">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="321.0" layoutY="117.0" text="Specify Below the names for the Input and output files of the program. " />
      <TitledPane collapsible="false" layoutX="49.0" layoutY="208.0" prefHeight="274.0" prefWidth="387.0" text="Input Files">
         <content>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="inputFile1Name" layoutX="39.0" layoutY="61.0" prefHeight="25.0" prefWidth="228.0" promptText="Select Input File 1" />
                  <Label layoutX="39.0" layoutY="35.0" text="Input File 1" />
                  <TextField fx:id="inputFile2Name" layoutX="39.0" layoutY="166.0" prefHeight="25.0" prefWidth="228.0" promptText="Select input File 2" />
                  <Label layoutX="39.0" layoutY="138.0" text="Input File 2" />
                  <Button fx:id="browseInFile1Button" layoutX="284.0" layoutY="61.0" mnemonicParsing="false" onAction="#handleInFile1BrowseButton" text="Browse..." />
                  <Button fx:id="browseInFile2Button" layoutX="284.0" layoutY="166.0" mnemonicParsing="false" onAction="#handleInFile2BrowseButton" text="Browse..." />
               </children>
            </Pane>
         </content>
      </TitledPane>
      <TitledPane collapsible="false" layoutX="556.0" layoutY="208.0" prefHeight="363.0" prefWidth="387.0" text="Output Files">
         <content>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="outFileName" layoutX="39.0" layoutY="46.0" prefHeight="25.0" prefWidth="228.0" promptText="Select Out File" />
                  <Label layoutX="39.0" layoutY="20.0" text="Out File" />
                  <TextField fx:id="outgFileName" layoutX="39.0" layoutY="125.0" prefHeight="25.0" prefWidth="228.0" promptText="Select Outg File" />
                  <Label layoutX="39.0" layoutY="97.0" text="Outg File" />
                  <Button fx:id="browseOutFileButton" layoutX="284.0" layoutY="46.0" mnemonicParsing="false" onAction="#handleOutFileBrowseButton" text="Browse..." />
                  <Button fx:id="brwoseOutgFileButton" layoutX="284.0" layoutY="125.0" mnemonicParsing="false" onAction="#handleOutgFileBrowseButton" text="Browse..." />
                  <TextField fx:id="out1FileName" layoutX="39.0" layoutY="205.0" prefHeight="25.0" prefWidth="228.0" promptText="Select Out1 File" />
                  <Label layoutX="39.0" layoutY="179.0" text="Out1 File" />
                  <Button fx:id="browseOut1FileButton" layoutX="284.0" layoutY="205.0" mnemonicParsing="false" onAction="#handleOut1FileBrowseButton" text="Browse..." />
                  <TextField fx:id="out2FileName" layoutX="39.0" layoutY="279.0" prefHeight="25.0" prefWidth="228.0" promptText="Select Out2 File" />
                  <Label layoutX="39.0" layoutY="252.0" text="Out2 File" />
                  <Button fx:id="browseOut2FileButton" layoutX="284.0" layoutY="279.0" mnemonicParsing="false" onAction="#handleOut2FileBrowseButton" text="Browse..." />
               </children>
            </Pane>
         </content>
      </TitledPane>
      <Button fx:id="resetButton" layoutX="137.0" layoutY="528.0" mnemonicParsing="false" onAction="#handleResetButton" text="Resset to Default">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/outline_restore_black_24dp.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="saveRunButton" layoutX="285.0" layoutY="528.0" mnemonicParsing="false" onAction="#handleSaveRunButton" text="Save &amp; Run">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/outline_play_circle_filled_white_black_24dp.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="backButton" layoutX="111.0" layoutY="73.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back">
         <graphic>
            <ImageView fitHeight="16.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/outline-arrow_back-24px.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Label layoutX="372.0" layoutY="134.0" text="You can browse through your computer to find locations." />
      <Button fx:id="nextButton" contentDisplay="RIGHT" layoutX="813.0" layoutY="73.0" mnemonicParsing="false" onAction="#handleUserParametersButtonTab" text="Next">
         <graphic>
            <ImageView fitHeight="16.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resources/icons/baseline_arrow_forward_black_24dp.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <ImageView fitHeight="38.0" fitWidth="38.0" layoutX="353.0" layoutY="78.0" opacity="0.72" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../resources/icons/outline_file_copy_black_24dpTitle.png" />
         </image>
      </ImageView>
      <ButtonBar layoutX="217.0" layoutY="22.0" prefHeight="40.0" prefWidth="527.0">
         <buttons>
            <Button fx:id="filesButtonTab" disable="true" mnemonicParsing="false" text="Files">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../resources/icons/outline_file_copy_black_24dp.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="userParametersButtonTab" mnemonicParsing="false" onAction="#handleUserParametersButtonTab" text="User Attributes">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../resources/icons/outline_group_black_24dp.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="profilesButtonTab" mnemonicParsing="false" onAction="#handleProfilesButtonTab" text="Profiles">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../resources/icons/baseline_dashboard_black_24dp.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="communitiesButtonTab" mnemonicParsing="false" onAction="#handleCommunitiesButtonTab" text="Communities &amp; Profiles">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../resources/icons/outline_device_hub_black_24dp.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="advancedButtonTab" mnemonicParsing="false" onAction="#handleAdvancedButtonTab" text="Advanced">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../resources/icons/outline_settings_black_24dp.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
